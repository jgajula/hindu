              <li><%= link_to "Link2", "/path2"  %></li>
              <li><%= link_to "Link3", "/path3"  %></li>
            </ul>
            <div id="user_nav">
              <% if user_signed_in? %>
                  Signed in as <%= current_user.email %>. Not you?
                  <%= link_to "Sign out", destroy_user_session_path, :method => :delete %>
              <% else %>
                  <%= link_to "Sign up", new_user_registration_path, "data-toggle" => "modal" %> or <%= link_to "sign in", new_user_session_path, "data-toggle" => "modal" %>
              <% end %>
            </div>
          </div><!--/.nav-collapse -->
        </div>
      </div>
    </div>

